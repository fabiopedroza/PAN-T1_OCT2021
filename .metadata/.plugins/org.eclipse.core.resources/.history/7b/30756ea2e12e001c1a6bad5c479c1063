package RefrigeradoresEncapsula;

public class Acao {
	
	public static void main(String[] args) {
		
		Refrigeradores arcond = new ArCondicionado();
		Refrigeradores ventil = new Ventilador();
		
		arcond.ligar();
		arcond.aumentar();
		arcond.desligar();
		
		ventil.ligar();
		ventil.aumentar();
		ventil.desligar();
		
	}

}
